{
  "name": "BlankE",
  "description": "Attempt at simple text editor",
  "repository": {
    "type": "git",
    "url": "git://github.com/xharris/texteditor.git"
  },
  "main": "main.js",
  "author": "Xavier Harris <xhh950@gmail.com> (http://xhhstuff.me)",
  "license": "CC-BY-NC-SA-4.0",
  "scripts": {
    "start": "grunt && electron .",
    "build": "electron-packager . BlankE --platform=win32 --asar-unpack-dir=data --arch=all --overwrite --out=../dist"
  },
  "webkit": {
    "plugin": true
  },
  "dependencies": {
    "electron-prebuilt": "^1.2.0",
    "fs-extra": "^0.26.5",
    "image-size": "^0.4.0",
    "mdi": "^1.6.50",
    "rimraf": "^2.5.4"
  },
  "devDependencies": {
    "electron-packager": "^7.4.0",
    "grunt": "^1.0.1",
    "grunt-contrib-less": "^1.3.0",
    "simple-grep": "0.0.1"
  },
  "build": {
    "appID": "blanke",
    "app-category-type": "public.app-category.developer-tools",
    "win": {
      "iconURL": "https://raw.githubusercontent.com/xharris/texteditor/master/src/build/icon.ico"
    }
  }
}
